{
    "name": "figma-to-elementor",
    "version": "1.0.0",
    "description": "Figma to Elementor Exporter",
    "main": "dist/code.js",
  "scripts": {
        "build": "esbuild src/code.ts --bundle --outfile=dist/code.js --target=es6 --loader:.md=text",
        "watch": "esbuild src/code.ts --bundle --outfile=dist/code.js --target=es6 --loader:.md=text --watch",
        "test": "vitest run"
    },
    "devDependencies": {
        "@figma/plugin-typings": "*",
        "esbuild": "^0.27.0",
        "typescript": "*",
        "vitest": "^1.6.0"
    }
}
